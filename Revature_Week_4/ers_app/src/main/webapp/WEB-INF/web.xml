<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>ers_app</display-name>
  
  
  <filter>
    <filter-name>originFilter</filter-name>
    <filter-class>com.revature.ers.util.OriginFilter</filter-class>
  </filter>
  
  <filter-mapping>
    <filter-name>originFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <servlet>
    <description></description>
    <display-name>UpdateReimbursementServlet</display-name>
    <servlet-name>UpdateReimbursementServlet</servlet-name>
    <servlet-class>com.revature.ers.servlets.UpdateReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateReimbursementServlet</servlet-name>
    <url-pattern>/updatereimbursements</url-pattern>
  </servlet-mapping>
  

</web-app>